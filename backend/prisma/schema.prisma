generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Costumer {
  id        String    @id @default(uuid())
  name      String
  email     String    @unique
  phone     String
  birth     DateTime
  address   String
  createdAt DateTime  @default(now()) @map("created_at")
  updatedAt DateTime? @updatedAt @map("updated_at")

  @@map("costumers")
}
